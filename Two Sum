 vector<int> twoSum(vector<int>& nums, int target) {
        
   vector<int>answer;
        int size = nums.size();
        for(int i=0 ; i<size; i++)
            for(int j=i+1; j<size; j++)
                if(nums[i]+nums[j]==target)
                {
                    answer.push_back(i);
                    answer.push_back(j);
                }
        return answer;
    }
};
